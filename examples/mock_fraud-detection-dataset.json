{
    "source": {
        "file_path": "hf://datasets/Phoenix21/mock_fraud-detection-dataset/transactions.csv",
        "schema_overrides": {
            "timestamp": "Datetime"
        }
    },
    "date_column": "timestamp",
    "target_column": "is_fraud",
    "columns_to_exclude": ["transaction_id"],
    "outliers": {
        "criterion": "IQR",
        "multiplier_iqr": 1.5,
        "threshold_z_score": 3.0
    },
    "markdown": {
        "name": "README.md",
        "css_style": "style.css",
        "float_precision": 4
    },
    "plotly": {
        "plot": {
            "mode": "lines+markers",
            "line": {
                "color": "dodgerblue",
                "dash": "dash",
                "width": 0.8
            },
            "marker": {
                "size": 6,
                "symbol": "circle",
                "color": "white",
                "line": {
                    "color": "dodgerblue",
                    "width": 0.5
                }
            }
        },
        "outliers": {
            "style": {
                "type": "rect",
                "xref": "x",
                "yref": "y",
                "fillcolor": "grey",
                "opacity": 0.05,
                "line": {
                    "width": 1.0,
                    "dash": "dot",
                    "color": "black"
                }
            }
        },
        "layout": {
            "template": "plotly_white",
            "margin": { "l": 20, "r": 20, "t": 30, "b": 20 },
            "showlegend": false,
            "height": 384,
            "height_scale_factor": 0.95,
            "width_scale_factor": 1.3
        },
        "grid": {
            "automargin": true,
            "gridcolor": "rgba(128,128,128,0.3)",
            "griddash": "dot",
            "gridwidth": 0.5
        },
        "subplots": {
            "horizontal_spacing": 0.05,
            "vertical_spacing": 0.065
        },
        "scale_factor": 2
    }
}
